pipeline {
  
  agent any
  
  stages {
    
    stage("build") {
      
      steps {
          echo "building test server"
          echo "that is great"
      }
    }
    
    stage("test") {
        
      steps {
          echo "testing test server"
      }
    }
    
    stage("deploy") {
      
      steps {
          echo "deploying test server"
      }
    }
  }
}

node {
     // groovy script
} 
  
